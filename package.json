{
  "name": "esmapv3",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start:frontend": "nx run frontend:start",
    "start:backend": "nx run backend:start",
    "build:frontend": "nx run frontend:build",
    "build:backend": "nx run backend:build",
    "test:frontend": "nx run frontend:test",
    "test:backend": "nx run backend:test",
    "dev": "nx run-many --target=start --projects=frontend,backend --parallel=true",
    "build:packages": "nx run-many --target=build --projects=types,graphql-schemas,database",
    "backend:dev": "cd apps/backend && pnpm run dev",
    "backend:build": "cd apps/backend && pnpm run build",
    "backend:start": "cd apps/backend && pnpm start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@nrwl/cli": "^15.9.3",
    "@nrwl/node": "^19.6.5",
    "@nrwl/react": "^19.6.5",
    "@nrwl/workspace": "^19.6.5",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "nx": "^19.6.5",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1",
  "dependencies": {
    "@esmapv3/database": "workspace:*",
    "@esmapv3/graphql-schemas": "workspace:*",
    "@esmapv3/types": "workspace:*"
  }
}
